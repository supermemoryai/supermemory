{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"outDir": "dist",
		"rootDir": "src",
		"baseUrl": "."
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
}
