		"ai": "4.0.16",
		"compromise": "^14.14.2",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.25.0",
		"drizzle-orm": "^0.34.1",
		"hono": "^4.6.4",
		"openai": "^4.68.4",
		"postgres": "^3.4.4",
		"uuid": "^11.0.1",
		"wrangler": "^3.111.0",
		"zod": "^3.23.8"
